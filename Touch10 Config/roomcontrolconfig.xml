<Extensions>
  <Version>1.4</Version>
  <Panel>
    <Icon>Lightbulb</Icon>
    <Type>Statusbar</Type>
    <Page>
      <Name>E-SDS Video Switch</Name>
      <Row>
        <Name>By Preset</Name>
      </Row>
      <Row>
        <Name>Choose Input</Name>
        <Widget>
          <WidgetId>LoadValue</WidgetId>
          <Type>GroupButton</Type>
          <Options>size=4</Options>
          <ValueSpace>
            <Value>
              <Key>1</Key>
              <Name>AppleTV</Name>
            </Value>
            <Value>
              <Key>2</Key>
              <Name>SpyCam</Name>
            </Value>
            <Value>
              <Key>3</Key>
              <Name>CableTV</Name>
            </Value>
          </ValueSpace>
        </Widget>
      </Row>
      <Row>
        <Name/>
      </Row>
      <Row>
        <Name>By Manual Entry</Name>
      </Row>
      <Row>
        <Name>Input</Name>
        <Widget>
          <WidgetId>InputChange</WidgetId>
          <Type>Spinner</Type>
          <Options>size=2</Options>
        </Widget>
        <Widget>
          <WidgetId>InputName</WidgetId>
          <Name>Text</Name>
          <Type>Text</Type>
          <Options>size=2</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Output</Name>
        <Widget>
          <WidgetId>OutputChange</WidgetId>
          <Type>Spinner</Type>
          <Options>size=2</Options>
        </Widget>
        <Widget>
          <WidgetId>OutputName</WidgetId>
          <Name>Text</Name>
          <Type>Text</Type>
          <Options>size=2</Options>
        </Widget>
      </Row>
      <Row>
        <Name/>
        <Widget>
          <WidgetId>Validation</WidgetId>
          <Type>Button</Type>
          <Options>size=1;icon=green</Options>
        </Widget>
      </Row>
      <Options/>
    </Page>
  </Panel>
</Extensions>